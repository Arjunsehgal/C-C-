#include <bits/stdc++.h>
using namespace std;

int main(){

// C++ STRINGS :

  string str1 = "Hello";
  cout<< str1<<endl;
  
  string str2;
  cin>>str2;
  cout<< str2 <<endl;

  string result = str1 + str2;
  cout<< result <<endl;

  if(str1 == str2){
    cout << "Strings are equal"<<endl;
  }else{
    cout<<"Strings are not equal"<<endl;
  }

  str1[0]='a';
  cout<< str1<<endl;
  cout<< str1.size()<<endl;

  for (int i=0;i<str1.size();i++){
    cout<< str1[i]<<endl;
  }
  
}
